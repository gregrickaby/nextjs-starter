{
  "name": "nextjs-starter",
  "version": "0.1.0",
  "description": "A slightly opinionated, yet bare-bones Next.js starter.",
  "homepage": "https://github.com/gregrickaby/nextjs-starter/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gregrickaby/nextjs-starter.git"
  },
  "author": "Greg Rickaby <gregrickaby@gmail.com>",
  "bugs": {
    "url": "https://github.com/gregrickaby/nextjs-starter/issues"
  },
  "private": true,
  "license": "GPL-2.0",
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "hamburger-react": "^2.2.0",
    "next": "^9.5.4-canary.13",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@storybook/addon-a11y": "^6.0.21",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/react": "^6.0.21",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "babel-loader": "^8.1.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^4.3.0",
    "lint-staged": ">=10",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.1.1",
    "react-is": "^16.13.1",
    "stylelint": "^13.7.1",
    "stylelint-config-standard": "^20.0.0",
    "tailwindcss": "^1.8.8",
    "typescript": "^4.0.2"
  },
  "scripts": {
    "build": "next build",
    "build-storybook": "build-storybook",
    "clean": "rm -rf .next",
    "dev": "yarn clean && yarn lint && next dev",
    "develop": "yarn dev",
    "format": "npx prettier --config=prettier.config.js '**/*.{js,jsx,ts,tsx,md,html,css,scss,json}' --write || true",
    "lint:js": "npx eslint --config=.eslintrc.js ./ --fix || true",
    "lint:css": "npx stylelint --config=stylelint.config.js '**/*.css' --fix || true",
    "lint": "yarn run format && yarn run lint:js && yarn run lint:css",
    "export": "next export",
    "start": "next start",
    "storybook": "start-storybook -p 6006"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
